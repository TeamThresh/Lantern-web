									block title
									| &nbsp;
									small
										block desc
										| &nbsp;
							page-toolbar
						block content
			right-header
			bottom-header
		//- BEGIN CORE PLUGINS
		script(src='/assets/theme2/global/plugins/jquery.min.js', type='text/javascript')
