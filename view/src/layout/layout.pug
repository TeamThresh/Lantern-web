		link(rel='shortcut icon', href='favicon.ico')
		block style
	body.page-container-bg-solid.page-header-fixed.page-sidebar-closed-hide-logo
		div#app
			top-header
			div.clearfix
			div.page-container
				left-header
				div.page-content-wrapper
