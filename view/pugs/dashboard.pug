extends layout.pug

block content
	card.col-xs-12.col-sm-5
		activity-map(ref='activityMap')
	div.col-xs-12.col-sm-3
		//- card(title='Device / OS')
		//- 	device-usage-graph
		card.col-xs-6.a(title='접속률')
			line-graph
		card.col-xs-6.a(title='복귀율')
			line-graph
		card(title='World Outbound Call')
			worldmap-graph
		card(title='Top Error')
			top-crash
block style
	style.
		.page-content > div {
			display: inline-block;
			vertical-align: top;
		}
		.a {
			display: inline-block; 
			float: none !important;
		}
prepend script