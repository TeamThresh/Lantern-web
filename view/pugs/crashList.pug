		div.card {
			border: 1px solid #3e3e3e;
			margin: 0px 0px 10px 0px;
			padding: 10px 0 0 10px;
		}
		div.card span.title {
			display: inline-block;
			/*margin: 10px 0 0 10px;*/
			color: #fff;
			font-size: 13px;
		}
